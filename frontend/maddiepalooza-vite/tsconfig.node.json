{
  "compilerOptions": {
    "lib": ["es2015", "dom"],
    "target": "es2015",
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "esnext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "esModuleInterop": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "vite.config.ts",
    "vite-env.d.ts",
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*"
  ],
  "exclude": ["node_modules"]
}
